{
    "source": "test/simple/src",
    "destination": "test/simple/build",
    "eraseIfExists": false,
    "visitors": ["./visitors/sep.js", "./visitors/jsmin.js", "test"],
    "packages": {
        "bootstrap.js": {
            "files": [
                "CORE.js",
                "CORE/Object.js",
                "CORE/Logging.js"
            ]
        },
        "logger.js": {
            "files": {
                "includes": ["CORE/log/*.js"]
            }
        },
        "utils.js": {
            "files": {
                "includes": ["CORE/**/*.js"],
                "excludes": ["CORE/*.js", "CORE/log/*"]
            }
        },
        "styles.css": {
            "visitors": ["./visitors/less.js"],
            "files": {
                "includes": ["**/*.less"]
            }
        }
    }
}